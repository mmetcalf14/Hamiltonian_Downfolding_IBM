title "Li2-REMOVE"
echo
start Li2-REMOVE

memory total 24 gb

geometry  units angstrom
symmetry c1
Li  0    0    0
Li  0    0    3.34125
end

basis spherical
   * library cc-pvtz
end

scf
thresh 1.0e-10
tol2e 1.0e-10
rhf
singlet
maxiter 200
end

tce
   ccsd
   maxiter 200
   thresh 1.e-6
   2eorb
   2emet 13
   tilesize 5
   freeze virtual 53
end

#set tce:qducc T
#set tce:nactv 7
set tce:printtol 0.001

task tce energy

