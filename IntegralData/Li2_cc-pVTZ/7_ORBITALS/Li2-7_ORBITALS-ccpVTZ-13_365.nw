title "Li2-REMOVE"
echo
start Li2-REMOVE

memory total 24 gb

geometry  units angstrom
symmetry c1
Li  0    0    0
Li  0    0    13.365
end

basis spherical
   * library cc-pvtz
end

scf
thresh 1.0e-10
tol2e 1.0e-10
rhf
singlet
maxiter 200
end

tce
   ccsd
   maxiter 200
   thresh 1.e-6
   2eorb
   2emet 13
   tilesize 5
end

set tce:qducc T
set tce:nactv 4

task tce energy

